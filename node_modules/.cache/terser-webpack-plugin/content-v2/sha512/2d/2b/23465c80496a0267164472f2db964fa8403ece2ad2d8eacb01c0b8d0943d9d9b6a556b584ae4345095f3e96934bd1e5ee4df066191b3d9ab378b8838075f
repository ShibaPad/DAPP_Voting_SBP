 type=\"text\" onKeyPress={(event) => {\n        if (!/[0-9]/.test(event.key)) {\n          event.preventDefault();\n        }\n        \n      }} value ={VoteAmount} onInput={e => setInput(e.target.value)\n      }\n        \n     \n            style={{ padding:5, borderRadius: 24,\n              border: \"4px dashed var(--accent)\",\n              textAlign: \"center\",\n                fontSize: 20, color: \"var(--accent)\", width: \"300px\"}}/>\n<s.SpacerSmall/>\n<s.TextTitle\n              style={{\n                textAlign: \"center\",\n                fontSize: 20,\n                color: \"var(--accent-text)\",\n              }}>\n                {feedback2}\n                </s.TextTitle>\n            <s.Container\n            flex={1}\n            ai={\"center\"} jc={\"center\"} fd={\"row\"}\n            style={{\n              backgroundColor: \"var(--primary)\",\n              padding: 5,\n              borderRadius: 24,\n              border: \"\",\n            }}\n            >\n                      <StyledButton\n                        disabled={Voting ? 1 : 0}\n                        onClick={(e) => {\n                          e.preventDefault();\n                          voteACCEPT();\n                          getData();\n                        }}\n                      >\n                        {Voting ? \"BUSY\" : \"VOTE ACCEPT\"}\n                      </StyledButton>\n                      &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n\n                      <StyledButton\n                        disabled={Voting ? 1 : 0}\n                        onClick={(e) => {\n                          e.preventDefault();\n                          voteREJECT();\n                          getData();\n                        }}\n                      >\n                        {Voting ? \"BUSY\" : \"VOTE REJECT\"}\n                      </StyledButton>\n                      </s.Container>\n                      <s.SpacerSmall/>\n    